package ${packageName}

import java.io.Serializable
import java.util.Date

class Hook implements Serializable {

	static transactional = true
	static hasMany = [roles:HookRole]
	
	${userClassName} user
	String name
	String url
	String format = 'JSON'
	String service
	Long attempts = 0
	Date dateCreated
	Date lastModified = new Date()

	static constraints = {
		user()
		name()
		url()
		format()
		service()
		attempts()
	}
}
